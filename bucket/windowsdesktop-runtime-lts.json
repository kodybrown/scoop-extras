    "installer": {
        "script": "Invoke-ExternalCommand \"$dir\\$fname\" -ArgumentList '/install', '/quiet', '/norestart' -RunAs | Out-Null"
    },
    "checkver": {
        "url": "https://dotnetcli.blob.core.windows.net/dotnet/WindowsDesktop/LTS/latest.version",
        "regex": "([\\d.]+)$"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
